{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node bin/www",
    "watch-scss": "node-sass --watch public/scss -o public/css",
    "watch-browserify": "watchify public/javascript/seed/main.js --standalone foo -o public/javascript/seed/bundle.js",
    "watch-litecoin": "watchify public/javascript/seed/litecoin.js --standalone litecoin -o public/javascript/seed/bundle3.js",
    "watch-swap": "watchify public/javascript/seed/swap.js --standalone swap -o public/javascript/seed/bundle2.js",
    "dev": "nodemon start & npm run watch-scss & npm run watch-browserify & npm run watch-swap & npm run watch-litecoin"
  },
  "dependencies": {
    "ajax": "0.0.4",
    "bip39": "^2.5.0",
    "bitcoin-core": "^2.0.0",
    "bitcoinjs-lib": "^3.3.2",
    "body-parser": "~1.18.2",
    "browser-request": "^0.3.3",
    "browserify": "^16.1.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "^4.16.3",
    "http": "0.0.0",
    "http-browserify": "^1.7.0",
    "jquery": "^3.3.1",
    "material-design-icons": "^3.0.1",
    "morgan": "~1.9.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.1",
    "request": "^2.85.0",
    "scss": "^0.2.4",
    "serve-favicon": "~2.4.5",
    "shapeshift.io": "^1.3.1",
    "socket.io": "^2.0.4",
    "watchify": "^3.11.0"
  }
}
